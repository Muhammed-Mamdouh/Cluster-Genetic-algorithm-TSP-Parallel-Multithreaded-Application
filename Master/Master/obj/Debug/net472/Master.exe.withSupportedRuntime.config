<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <!--Configuration of Genetic Algorithm-->
    <add key="SelectionMethod" value="Roulette" />
    <add key="CrossOverMethod" value="CrossOver2Points" />
    <add key="MutationMethod" value="Swaping" />
    <add key="EvalMethod" value="Distance" />
    <!--Data Path : same directory of .exe file-->
    <add key="data_path" value="pma343.csv" />
    <!--HyperParameter of Genetic Algorithm-->
    <add key="PopulationSize" value="600" />
    <add key="tournamentSampleSize" value="0.05" />
    <add key="winningProbability" value="0.7" />
    <add key="mutationProbability" value="0.07" />
    <add key="ElitesRatio" value="0.1" />
    <add key="isClosedLoop" value="true" />
    <add key="StartingCity" value="1" />
    <add key="EndingCity" value="1" />
    <!--Islands is where to write Island names, bins is for Entropy Calculations-->
    <add key="Islands" value="I1,I2,I3" />
    <add key="numberOfGenerationsBetweenMergers" value="200" />
    <add key="BinsNo" value="20" />
    <!--Parallelism Settings (tpl)-->
    <add key="MaxDegreeOfParallelism" value="-1" />
    <add key="BoundedCapacity" value="-1" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Threading.Tasks.Extensions" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.2.0.1" newVersion="4.2.0.1" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>